<AnchorPane fx:id="registerformcontext" prefHeight="449.0" prefWidth="680.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.devstack.edu.controller.RegisterFormController">
   <children>
      <ImageView fitHeight="482.0" fitWidth="269.0" layoutX="-32.0" layoutY="-30.0" pickOnBounds="true" smooth="false">
