      </ImageView>
      <TextField fx:id="txtFristName" layoutX="250.0" layoutY="78.0" prefHeight="34.0" prefWidth="194.0" promptText="Frist Name" style="-fx-text-fill: #10439F;" />
      <PasswordField fx:id="txtPassword" layoutX="249.0" layoutY="188.0" prefHeight="34.0" prefWidth="411.0" promptText="password" style="-fx-text-fill: #10439F;" />
      <Label layoutX="444.0" layoutY="328.0" text="OR">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Button layoutX="249.0" layoutY="263.0" mnemonicParsing="false" onAction="#SignupbtnOnAction" prefHeight="42.0" prefWidth="411.0" style="-fx-background-color: #008DDA;" text="Sign up" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
