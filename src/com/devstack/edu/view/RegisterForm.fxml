         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <TextField fx:id="txtLastname" layoutX="455.0" layoutY="78.0" prefHeight="34.0" prefWidth="204.0" promptText="Last Name" style="-fx-text-fill: #10439F;" />
      <TextField fx:id="txtEmail" layoutX="248.0" layoutY="133.0" prefHeight="34.0" prefWidth="411.0" promptText="email" style="-fx-text-fill: #10439F;" />
   </children>
</AnchorPane>
